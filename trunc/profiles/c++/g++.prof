[info]
compiler=g++
comment=GNU project C and C++ compiler

[compile]
default=-O2 --static -o $output$ -I /usr/include/kuzya/ $options$ $source$
object=-O2 -c -o $output$ -I /usr/include/kuzya/ $options$ $source$
static_lib=
dynamic_lib=
alternative=

[errors]
error_messages_1=1:3:(\\d+):\\s(.*)error:\\s(.*)
error_messages_2=1:4:(\\d+):(\\d+):\\s(.*)error:\\s(.*)

[warnings]