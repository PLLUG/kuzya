[info]
compiler=g++
language=c,c++
comment=GNU project C and C++ compiler

[compile]
default=-O2 -o $output$ -I /usr/include/kuzya/ $options$ $source$
alternative=-o $output$ $options$ $source$ 

[errors]
error_messages_1=1:3:(\\d+):\\s(.*)error:\\s(.*)
error_messages_2=1:4:(\\d+):(\\d+):\\s(.*)error:\\s(.*)

[warnings]