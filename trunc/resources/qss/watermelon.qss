QToolButton
{ 
	border-radius: 5px;
}
QToolButton#browseButton
{
	background:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #fafafa, stop: 0.4 #f4f4f4,
                                 stop: 0.5 #e7e7e7, stop: 1.0 #fafafa);
	padding:2;
	border: 2px groove grey;
	border-radius:5;
	
}
QToolButton#browseButton:hover
{
	border-bottom-color:black;
	border-style:inset;
	color:white;
	background:qlineargradient(spread:pad, x1:0.484, y1:0, x2:0.443273, y2:1, stop:0 rgba(68, 68, 68, 255), stop:0.960227 rgba(198, 198, 198, 255));
	padding-left:1px;
	padding-top:1px;

}
QToolButton#browseButton:focus
{
border-color:orange;
	border-style:inset;
	padding-left: 2px;
    padding-top: 2px;
}
QToolButton:pressed
{
	border: 1px solid white;
	background:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #dadbde, stop: 1 #f6f7fa);

}
QToolButton:checked
{
	border: 1px solid #8f8f91;
	background:qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:1.227, fx:0.5, fy:0.5, stop:0.0113636 rgba(244, 242, 230, 255), stop:0.164773 rgba(218, 216, 205, 255), stop:0.528409 rgba(173, 171, 163, 255), stop:0.971591 rgba(176, 175, 166, 255));
}
QToolButton:hover
{
	border: 1px solid #8f8f91;
	background:qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5, stop:0 rgba(255, 239, 254, 255), stop:1 rgba(255, 187, 178, 255));/*qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.668, fx:0.5, fy:0.5, stop:0.0113636 rgba(230, 228, 216, 255), stop:0.164773 rgba(218, 216, 205, 255), stop:0.528409 rgba(173, 171, 163, 255), stop:0.909091 rgba(118, 117, 111, 255));
*/
}
QToolBar:horizontal
{
	
	background-color: qlineargradient(spread:pad, x1:0.528, y1:0, x2:0.534, y2:1, stop:0.0284091 rgba(71, 207, 132, 255), stop:0.534091 rgba(131, 162, 145, 255), stop:1 rgba(67, 182, 100, 255));
	
}
QToolBar:vertical
{
	background-color:qlineargradient(spread:reflect, x1:0, y1:0.5, x2:0.500318, y2:0.506, stop:0 rgba(61, 134, 97, 255), stop:0.948864 rgba(175, 255, 209, 255))
}
QToolBar::handle
{
	background-color:qlineargradient(spread:repeat, x1:0.511364, y1:0.445, x2:0.511, y2:0.392, stop:0.0113636 rgba(105, 194, 157, 255), stop:0.511364 rgba(226, 255, 171, 255), stop:1 rgba(105, 194, 157, 255));
	
}
QStatusBar
