	background-color:qlineargradient(spread:pad, x1:0.511, y1:0, x2:0.517045, y2:1, stop:0 rgba(103, 194, 164, 255), stop:0.465909 rgba(99, 153, 77, 255), stop:0.931818 rgba(108, 140, 127, 255));
