}


QTabWidget::pane 
{     		
	border-top: 2px solid #C2C7CB;
	background:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #fafafa, stop: 0.4 #f4f4f4,
                                 stop: 0.5 #e7e7e7, stop: 1.0 #fafafa);/*rgb(226, 226, 226);*/
	border: 2px solid black;
	border-radius:5;
}
QTabWidget::tab-bar 
{
    left: 5px; 
}

 
 QTabBar::tab {
     background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #E1E1E1, stop: 0.4 #DDDDDD,
                                 stop: 0.5 #D8D8D8, stop: 1.0 #D3D3D3);
     border: 2px solid #C4C4C3;
     border-bottom-color: #C2C7CB; /* same as the pane color */
     border-top-left-radius: 4px;
     border-top-right-radius: 4px;
     min-width: 8ex;
     padding: 2px;
	 
 }

 QTabBar::tab:selected, QTabBar::tab:hover {
     background:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #fafafa, stop: 0.4 #f4f4f4,
                                 stop: 0.5 #e7e7e7, stop: 1.0 #fafafa);
 }

 QTabBar::tab:selected {
     border-color: #9B9B9B;
     border-bottom-color: rgb(226, 226, 226); 
 }

 QTabBar::tab:!selected {
     margin-top: 2px; 
	 
 }


/*-----------------------------SCROLL_BAR--------------------------------------------------------*/

QScrollBar:vertical {
    border: 2px solid rgb(119, 156, 131);
    background:rgb(220, 255, 229);
    width: 15px;
    margin: 22px 0 22px 0;
	color:white;
	
  }
  QScrollBar::handle:vertical {
    border: 1px solid green;
	border-radius: 5; 
	background: qlineargradient(spread:pad, x1:1, y1:0.501636, x2:0, y2:0.500091, stop:0.0113636 rgba(105, 194, 157, 255), stop:0.511364 rgba(226, 255, 171, 255), stop:1 rgba(105, 194, 157, 255));
    min-height: 20px;
  }
 QScrollBar::add-line:vertical 
 {
    border: 2px solid rgb(8, 102, 35);
    border-radius: 6;
    background:qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5, stop:0 rgba(25, 102, 40, 255), stop:0.479904 rgba(255, 147, 147, 255), stop:0.522685 rgba(255, 255, 255, 255), stop:1 rgba(206, 198, 255, 255));
	height: 20px;
	margin-bottom:1;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

  QScrollBar::sub-line:vertical {
    border: 2px solid rgb(8, 102, 35);
	border-radius: 6;
   	background:qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5, stop:0 rgba(25, 102, 40, 255), stop:0.479904 rgba(255, 147, 147, 255), stop:0.522685 rgba(255, 255, 255, 255), stop:1 rgba(206, 198, 255, 255));
	height: 20px;
	margin-top:1;
    subcontrol-position: top;
    subcontrol-origin: margin;
  }

 QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical 
 {
    background: none;
}

 QScrollBar:horizontal 
 {
    border: 2px solid rgb(119, 156, 131);
    background: rgb(220, 255, 229);
    height: 15px;
    margin: 0px 20px 0 20px;
}

  QScrollBar::handle:horizontal {
    border: 1px solid green;
	border-radius: 5; 
	background: qlineargradient(spread:pad, x1:0.466227, y1:0.013, x2:0.473, y2:0.982955, stop:0.0113636 rgba(105, 194, 157, 255), stop:0.511364 rgba(226, 255, 171, 255), stop:1 rgba(105, 194, 157, 255));
    min-width: 20px;
  }
 QScrollBar::add-line:horizontal 
 {
    border: 2px solid rgb(8, 102, 35);
    border-radius: 6;
    background:qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5, stop:0 rgba(25, 102, 40, 255), stop:0.479904 rgba(255, 147, 147, 255), stop:0.522685 rgba(255, 255, 255, 255), stop:1 rgba(206, 198, 255, 255));
	width: 20px;
	margin-left:2;
    subcontrol-position: right;
    subcontrol-origin: margin;
}

  QScrollBar::sub-line:horizontal {
    border: 2px solid rgb(8, 102, 35);
	border-radius: 6;
   	background:qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5, stop:0 rgba(25, 102, 40, 255), stop:0.479904 rgba(255, 147, 147, 255), stop:0.522685 rgba(255, 255, 255, 255), stop:1 rgba(206, 198, 255, 255));
	width: 20px;
	margin-right:2;
    subcontrol-position: left;
    subcontrol-origin: margin;
  }
QScrollBar::handle:hover
{
	border-color:white;
}
QScrollBar::handle:pressed
{
	border-color:grey;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
 {
    background: none;
}
QScrollBar::add-line:hover
{
	border-color:grey;
}
QScrollBar::add-line:pressed
{
	border-color:white;
	padding-top:-2;
	padding-left:-2;
}
QScrollBar::sub-line:hover
{
	border-color:grey;
}
QScrollBar::sub-line:pressed
{
	border-color:white;
	padding-top:-2;
	padding-left:-2;
}
/*-----------------------QPUSH_BUTTON---------------------------------*/

QPushButton
{
	background:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #fafafa, stop: 0.4 #f4f4f4,
                                 stop: 0.5 #e7e7e7, stop: 1.0 #fafafa);
	padding:3;
	border: 2px groove grey;
	border-radius:5;
	min-width:30px;
	
	}
QPushButton:hover
{
	border-bottom-color:red;
	border-style:inset;
	color:white;
	background:qlineargradient(spread:pad, x1:0.511, y1:0, x2:0.517045, y2:1, stop:0 rgba(61, 115, 97, 255), stop:0.988636 rgba(164, 200, 166, 255));
	padding-left:1px;
	padding-top:1px;
}
QPushButton:pressed
{
	color:white;
	border-color:darkgrey;
	background:qlineargradient(spread:pad, x1:1, y1:0.983, x2:0, y2:0.028, stop:0 rgba(80, 162, 135, 255), stop:0.988636 rgba(164, 200, 166, 255));
	border-style: outset;
	padding-left: 5px;
    padding-top: 5px;
}
QPushButton:focus
{
	border-color:orange;
	border-style:inset;
	padding-left: 2px;
    padding-top: 2px;
}
