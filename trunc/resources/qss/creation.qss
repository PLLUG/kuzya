QMenuBar
{
	border: 1px ridge gray;
	background-color: qlineargradient(spread:pad, x1:0.479, y1:0, x2:0.484, y2:0.994318, stop:0 rgba(227, 227, 227, 255), stop:1 rgba(209, 209, 209, 255));
}
QMenuBar::item::selected
{
	background-color:qlineargradient(spread:pad, x1:0.479, y1:0, x2:0.484, y2:0.994318, stop:0 rgba(227, 227, 227, 255), stop:1 rgba(209, 209, 209, 255));

}
QMenuBar::item::pressed
{
	background-color:#7C746A;
	color:white;
}
QMenu::item::checked
{
	background-color: lightgray;
	
}
QMenu
{

  border: 1px solid gray;
  border-radius:0px;
  color:black;
  /*background-color:#FFDE7B*/
}
QMenu::item:selected
{
  color:white;
  /*background-color:#FFC004;*/
}
QMenu::separator
{
  height:1px;
  background:gray;
 
}
QToolButton
{ 
	border-radius: 5px;
}
QToolButton#browseButton
{
	background:qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                 stop: 0 #fafafa, stop: 0.4 #f4f4f4,
                                 stop: 0.5 #e7e7e7, stop: 1.0 #fafafa);
	padding:2;
	border: 2px groove grey;
	border-radius:5;
	
}
QToolButton#browseButton:hover
{
	border-bottom-color:black;
	border-style:inset;
	color:white;
	background:qlineargradient(spread:pad, x1:0.484, y1:0, x2:0.443273, y2:1, stop:0 rgba(68, 68, 68, 255), stop:0.960227 rgba(198, 198, 198, 255));
	padding-left:1px;
	padding-top:1px;

}
QToolButton#browseButton:focus
{
border-color:orange;
	border-style:inset;
	padding-left: 2px;
    padding-top: 2px;
}
QToolButton:pressed
{
	border: 1px solid white;
	background:qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(255, 255, 255, 255), stop:1 rgba(191, 191, 191, 222));

}
QToolButton:checked
{
	border: 1px solid #8f8f91;
	background:qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:1.227, fx:0.5, fy:0.5, stop:0.0113636 rgba(244, 242, 230, 255), stop:0.164773 rgba(218, 216, 205, 255), stop:0.528409 rgba(173, 171, 163, 255), stop:0.971591 rgba(176, 175, 166, 255));
}
QToolButton:hover
{
	border: 1px solid #8f8f91;
	background:qlineargradient(spread:repeat, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(255, 255, 255, 35), stop:1 rgba(255, 255, 255, 53));

}
QToolBar:horizontal
{
	border: 1px solid darkgray;
	background-color: qlineargradient(spread:pad, x1:0.479, y1:0, x2:0.489, y2:0.994318, stop:0 rgba(126, 126, 126, 255), stop:0.647959 rgba(85, 85, 85, 255), stop:1 rgba(85, 85, 85, 255));
	
}
QToolBar:vertical
{
	
	background-color:qlineargradient(spread:pad, x1:0, y1:0.5, x2:1, y2:0.5, stop:0 rgba(71, 71, 71, 255), stop:0.229592 rgba(85, 85, 85, 255), stop:1 rgba(116, 116, 116, 255));
