<?xml version="1.0" encoding="UTF-8"?>
<compilersettings name="gcc" info="GNU C Compiler" lang="c++" filter="*.cpp,*.c">
	<modes>
		<default params="-o $output$ $options$ $source$" />
		<object params="-c -o $output$ $options$ $source$" />
		<staticlib params="" />
		<dylib params="" />
		<alternative params="" />
	</modes>
	<errors>
		<msg ln="0" txt="1" visible="1" regexp="(In\sfile\s.*:)"/>
		<msg ln="0" txt="1" visible="1" regexp="(.*ld.*:\s.*:\s.*)"/>
		<msg ln="0" txt="1" visible="1" regexp="(collect2:\s.*)"/>
		<msg ln="0" txt="1" visible="1" regexp="(error:\sunrecognized\s.*)"/>
		<msg ln="1" txt="4" visible="1" regexp="(\d+):(\d+):\s(.*)error:\s(.*)"/>
		<msg ln="1" txt="3" visible="1" regexp="(\d+):\s(.*)error:\s(.*)"/>
		<msg ln="1" txt="3" visible="1" regexp="(\d+):\s(.*)"/>
	</errors>
	<warnings>
		<msg ln="0" txt="1" visible="1" regexp="(In\sfile\s.*:)"/>
		<msg ln="1" txt="4" visible="1" regexp="(\d+):(\d+):\s(.*)warning:\s(.*)"/>
	</warnings>
</compilersettings>